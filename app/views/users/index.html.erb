<h3>All Clients</h3>
<table class= "table">
<thead>
<tr>
<th>No</th>
<th>Title</th>
<th>Name</th>
<th>Mobile No</th>
<th>Email</th>
<th>Address</th>
<th>Birthday</th>
</tr>
</thead>

<tbody>
<%@users.each_with_index do |u,index|%>
<tr>
<td><%=index+1%></td>
<td><%=u.title%></td>
<td><%=u.name%></td>
<td><%=u.mobile_no%></td>
<td><%=u.email%></td>
<td><%=u.address%></td>
<td><%=u.birthday%></td>
</tr>
<%end%>
</tbody>
</table>