

<div class="form-group">
  <%= form.label :title %>
  <%= form.select :title,options_for_select(User.titles.keys),{:prompt => "Select Title"},{class: "form-control",required: true}%>
</div>

<div class="form-group">
  <%= form.label :name %>
  <%= form.text_field :name,class: "form-control",required: true %>
</div>

<div class="form-group">
  <%= form.label :email %>
  <%= form.email_field :email,type: "email",class: "form-control",required: true %>
</div>

<div class="form-group">
  <%= form.label :mobile_number %>
  <%= form.text_field :mobile_no,class: "form-control",required: true %>
</div>

<%=form.hidden_field :password, :value => "mykey2017"%>

